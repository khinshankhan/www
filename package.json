{
  "name": "anchorage",
  "description": "Source code for anchorage",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Khinshan Khan <shan@uptogood.dev>",
  "keywords": [],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "develop": "pnpm run develop --filter www",
    "build": "pnpm run build --filter www",
    "serve": "pnpm run serve --filter www",
    "clean": "pnpm run clean --filter www",
    "typecheck": "pnpm m run typecheck --workspace-concurrency 1",
    "lint": "eslint . --ignore-path .gitignore --ignore-path .prettierignore --ext .ts,.tsx,.js",
    "lint:fix": "pnpm run lint -- --fix",
    "format": "prettier \"**/*.{md,mdx,json,yaml,ts,tsx,js,jsx}\" --write",
    "prepare": "husky install",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "add": "^2.0.6",
    "babel-jest": "^28.1.3",
    "babel-preset-gatsby": "^2.20.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphql": "15",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "pnpm": "^7.9.0",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
